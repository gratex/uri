{
    "plugins": [],
    "recurseDepth": 10,
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "source":{
        "include": ["src/_uri.js",  "src/uri.js", "src/index.js"]
    },
    "access": "public",
    "opts": {
        "template": "node_modules/docdash"
      },
        "docdash": {
            "static": true,  
            "sort": true
        }
    
}
